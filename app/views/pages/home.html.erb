<div class="background_home" style="background-image:  url(' <%= cl_image_path("group_of_young_people_eating.jpg_z853ab.jpg") %> ');">
  <div class="container">

    <% if user_signed_in? %>
      <%= link_to destroy_user_session_path, method: :delete do %>
        <i class="fa fa-sign-out"></i>  <%= t(".sign_out", default: "Log out") %>
      <% end %>
    <% else %>
      <div class="#">
        <%= link_to "Sign up", "/users/sign_up", class: ".sign_up" %>
        <%= link_to "Login", "/users/sign_in", class: ".sign_in" %>
      </div>
    <% end %>

    <div class="home_icons">
      <%= link_to 'Groups', groups_path %>
      <%= link_to 'Create group', new_group_path %>
      <%= link_to 'Dashboard', dashboard_path %>
      <%= link_to 'Join group', new_participation_path %>
    </div>

  </div>
</div>

